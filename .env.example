# Public URL, used for OG Image when running nuxt generate
NUXT_PUBLIC_SITE_URL=

# Dev.to API Key (for dynamic follower count on stats page)
# Get from: https://dev.to/settings/extensions
DEVTO_API_KEY=

# GitHub Personal Access Token (for dynamic contribution stats)
# Get from: https://github.com/settings/tokens
# Required scopes: public_repo (for GraphQL contribution stats)
GITHUB_TOKEN=
